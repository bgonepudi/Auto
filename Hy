# Specify the path to the JSON file
$jsonPath = "C:\path\to\your\output.json"

# Read the JSON file content
$jsonContent = Get-Content -Path $jsonPath -Raw

# Convert the JSON content to a PowerShell object
$parsedJson = $jsonContent | ConvertFrom-Json

# Output the parsed JSON content
$parsedJson

# To access specific properties in the JSON object, you can use dot notation
# For example, if the JSON contains an array of objects, you can iterate over it
foreach ($item in $parsedJson) {
    Write-Host "Property1: $($item.Property1), Property2: $($item.Property2)"
}
