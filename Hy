# Specify the path to the JSON file
$jsonPath = "C:\path\to\your\output.json"

# Check if the file exists
if (Test-Path -Path $jsonPath) {
    try {
        # Read the JSON file content
        $jsonContent = Get-Content -Path $jsonPath -Raw
        Write-Host "JSON content successfully read from file."

        # Convert the JSON content to a PowerShell object
        $parsedJson = $jsonContent | ConvertFrom-Json
        Write-Host "JSON content successfully parsed."

        # Output the parsed JSON content for inspection
        $parsedJson | Format-List -Property *

        # To access specific properties in the JSON object, you can use dot notation
        # For example, if the JSON contains an array of objects, you can iterate over it
        if ($parsedJson -is [System.Collections.IEnumerable]) {
            foreach ($item in $parsedJson) {
                # Output the entire item for detailed inspection
                Write-Host "Item: $($item | Format-List -Property *)"
                
                # Access specific properties
                if ($item.PSObject.Properties["Property1"] -and $item.PSObject.Properties["Property2"]) {
                    Write-Host "Property1: $($item.Property1), Property2: $($item.Property2)"
                } else {
                    Write-Host "Properties 'Property1' and/or 'Property2' not found in the item."
                }
            }
        } else {
            Write-Host "Parsed JSON is not an array."
        }
    } catch {
        Write-Error "Failed to parse JSON content: $_"
    }
} else {
    Write-Error "JSON file not found at path: $jsonPath"
}
